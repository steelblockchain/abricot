        "@tensorflow/tfjs-node": "^4.6.0",
        "fastify": "^4.17.0",
        "frida": "^16.0.19",
        "gun": "^0.2020.1239",
        "jsonpath-plus": "^7.2.0",
        "node-cache": "^5.1.2",
        "openai": "^3.2.1",
        "readline": "^1.3.0",
